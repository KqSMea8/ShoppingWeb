<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="web.entity.ShippingAddress" table="item_shippingaddress">
		<id name="aid" column="aid">
			<generator class="native"/>
		</id>
		<property name="uid"/>
		<property name="consignee"/>
		<property name="phone"/>
		<property name="postcode"/>
		<property name="remark"/>
		<property name="shippingaddress"/>
		<property name="createdUser" column="created_user"/>
		<property name="createdTime" column="created_time"/>
		<property name="modifiedUser" column="modified_user"/>
		<property name="modifiedTime" column="modified_time"/>
	</class>
</hibernate-mapping>