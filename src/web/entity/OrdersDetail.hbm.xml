<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="web.entity.OrdersDetail" table="item_ordersdetail">
		<id name="id" column="id">
			<generator class="native"/>
		</id>
		<property name="itemsNum"/>
		<many-to-one name="orders" class="web.entity.Orders" column="orders_id"/>
		<many-to-one name="items" class="web.entity.Items" column="items_id"/>
	</class>
</hibernate-mapping>